    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "fp-ts": "^2.9.5",
    "io-ts": "^2.2.14",
    "io-ts-types": "^0.5.13",

  "scripts": {
    "build": "concurrently --kill-others-on-fail \"yarn build-server\" \"yarn build-client\"",
    "build-client": "craco build",
    "build-server": "tsc --project tsconfig.server.json",
    "client": "craco start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "eject": "react-scripts eject",
    "heroku-postbuild": "echo 'Nothing to do'",
    "server": "npx nodemon server.js",
    "start": "node build-server/server/index.js",
    "test": "craco test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000/",
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/postcss7-compat": "^2.1.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/react-test-renderer": "^17.0.1",
    "autoprefixer": "9",
    "concurrently": "^4.0.1",
    "postcss": "7",
    "react-test-renderer": "^17.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  }
